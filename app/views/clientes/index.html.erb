<h1>Lista de Clientes</h1>

<table>
    <thead>
        <tr>
            <th>ID Cliente</th>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Email</th>
            <th>Telefono</th>
        </tr>
    </thead>
    <tbody>
        <% @clientes.each do |cliente| %>
            <tr>
                <td> <%= cliente.id %> </td>
                <td> <%= cliente.nombre %> </td>
                <td> <%= cliente.apellido %> </td>
                <td> <%= cliente.email %> </td>
                <td> <%= cliente.telefono %> </td>
            </tr>
        <% end %>
    </tbody>
</table>