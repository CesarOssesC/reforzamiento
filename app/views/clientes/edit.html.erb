<h1>Editar Clientes</h1>

<div class="col-6">
    <%= form_with(model: @cliente) do |form| %>
        <div>
            <%= form.label :nombre %>
            <%= form.text_field :nombre %>
        </div>
        <div>
            <%= form.label :apellido %>
            <%= form.text_field :apellido %>
        </div>
        <div>
            <%= form.label :email %>
            <%= form.text_field :email %>
        </div>
        <div>
            <%= form.label :telefono %>
            <%= form.text_field :telefono %>
        </div>
        <div>
            <%= form.submit %>
        </div>
    <% end %>
</div>